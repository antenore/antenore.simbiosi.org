{
  "$schema": "https://beta.frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "image",
          "name": "image",
          "type": "image"
        },
        {
          "title": "headerImage",
          "name": "headerImage",
          "type": "image"
        },
        {
          "title": "projects",
          "name": "projects",
          "type": "boolean"
        },
        {
          "title": "hidden",
          "name": "hidden",
          "type": "boolean"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "category",
          "name": "category",
          "type": "categories"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "externalLink",
          "name": "externalLink",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.content.pageFolders": [
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "title": "pages",
      "path": "[[workspace]]/pages"
    }
  ],
  "frontMatter.git.enabled": false,
  "frontMatter.website.host": "https://antenore.simbiosi.org"
}